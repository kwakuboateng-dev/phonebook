<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= contact.phone_number %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= contact.address %>
  </p>

</div>
